{
    "compilerOptions": {
        "baseUrl":".",
        "paths": {
            "@angular/common":["trainingapp/jspm_packages/npm/@angular/common@2.0.0-rc.1"],
            "@angular/compiler":["trainingapp/jspm_packages/npm/@angular/compiler@2.0.0-rc.1"],
            "@angular/core":["trainingapp/jspm_packages/npm/@angular/core@2.0.0-rc.1"],
            "@angular/http":["trainingapp/jspm_packages/npm/@angular/http@2.0.0-rc.1"],
            "@angular/platform-browser-dynamic":["trainingapp/jspm_packages/npm/@angular/platform-browser-dynamic@2.0.0-rc.1"],
            "@angular/platform-browser-dynamic/*":["trainingapp/jspm_packages/npm/@angular/platform-browser-dynamic@2.0.0-rc.1/*"],
            "@angular/platform-browser":["trainingapp/jspm_packages/npm/@angular/platform-browser@2.0.0-rc.1"],
            "@angular/router-deprecated":["trainingapp/jspm_packages/npm/@angular/router-deprecated@2.0.0-rc.1"],
            "@angular/router":["trainingapp/jspm_packages/npm/@angular/router@2.0.0-rc.1"],
            "@angular/upgrade":["trainingapp/jspm_packages/npm/@angular/upgrade@2.0.0-rc.1"],
            "rxjs/*":["trainingapp/jspm_packages/npm/rxjs@5.0.0-beta.6/*"]
            // "google-code-prettify/*":["trainingapp/jspm_packages/npm/google-code-prettify@1.0.1/*"]
        },        
       "target": "es5",
        "moduleResolution": "node",
        "module": "system",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "preserveConstEnums": true,
        "noImplicitAny": true,
        "sourceMap": true
    },
    "exclude": [
        "node_modules", "trainingapp/jspm_packages", "typings"
    ]
}
